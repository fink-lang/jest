{process} = import `./transform`


describe:: process, fn:

  it:: `compiles fink`, fn:
    {code, map} = process:: `foo = 1234`, `foobar.fnk`, {rootDir: `./src`}

    expect({code, map}).toMatchSnapshot()


  it:: `handles errors`, fn:


    expect(
      fn: process:: `123 = foo`, `foobar.fnk`, {rootDir: `./src`}
    ).toThrow(`
      foobar.fnk:1:0
      1| 123 = foo
         ^

      Unable to transform 'assign ='.
    `)

  undefined
